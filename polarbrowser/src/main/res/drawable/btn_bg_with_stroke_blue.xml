<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/color_box_item_1"/>
            <stroke android:width="@dimen/common_padding_0_5" android:color="@color/login_btn_press_color"></stroke>
            <corners android:radius="@dimen/color_box_2dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/color_box_item_1"/>
            <stroke android:width="@dimen/common_padding_0_5" android:color="@color/login_default_color"></stroke>
            <corners android:radius="@dimen/color_box_2dp"/>
        </shape>
    </item>

</selector>